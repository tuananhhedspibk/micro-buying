{
  "name": "order-svc",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:generate": "yarn typeorm migration:generate -d ./apps/query/src/infrastructure/config/rdb.ts ./apps/query/src/infrastructure/migrations/$npm_config_name",
    "migration:revert": "yarn typeorm -d ./apps/query/src/infrastructure/config/rdb.ts migration:revert",
    "migration:run": "yarn typeorm migration:run -d ./apps/query/src/infrastructure/config/rdb.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "proto:install": "yarn add git+github:tuananhhedspibk/micro-buying-protos.git",
    "proto:command:order-command": "protoc --plugin=node_modules/.bin/protoc-gen-ts_proto -I=./node_modules/micro-buying-protos --ts_proto_out=apps/command/src/common/ node_modules/micro-buying-protos/proto/order-command.proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb",
    "proto:command:item-command": "protoc --plugin=node_modules/.bin/protoc-gen-ts_proto -I=./node_modules/micro-buying-protos --ts_proto_out=apps/command/src/common/ node_modules/micro-buying-protos/proto/item-command.proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb",
    "proto:command:item-query": "protoc --plugin=node_modules/.bin/protoc-gen-ts_proto -I=./node_modules/micro-buying-protos --ts_proto_out=apps/command/src/common/ node_modules/micro-buying-protos/proto/item-query.proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.5",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/cqrs": "^10.2.6",
    "@nestjs/microservices": "^10.2.7",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "kafkajs": "^2.2.4",
    "micro-buying-protos": "git+github:tuananhhedspibk/micro-buying-protos.git",
    "mysql": "^2.18.1",
    "nestjs-event-sourcing": "^1.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "ts-proto": "^1.162.1",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
